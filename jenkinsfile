node(){
    stage('checkout'){
        checkout scm
    }
    stage('Build'){
        sh "sh variable.sh"
    }
    stage('Notify'){
        sh " echo output should be hello World"
    }
    post {
        success {
            echo "success"
        }
        failure {
            echo "work hard"
        }
        always{
            deletedi()
        }
    }
}